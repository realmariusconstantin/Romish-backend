{
  "name": "romish-backend",
  "version": "1.0.0",
  "description": "Backend API for Romish.gg - CS2 10-man matchmaking system",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "seed": "node scripts/seedUsers.js",
    "clear:queue": "node scripts/clearQueue.js",
    "set-admin": "node scripts/setAdmin.js",
    "list-users": "node scripts/listUsers.js",
  "remove-simplayers-cleanup": "node scripts/removeSimPlayersAndCleanup.js",
    "delete-simplayers": "node scripts/deleteAllSimPlayers.js",
    "delete-simplayer-pattern": "node scripts/deleteSimPlayerPattern.js",
    "smoke": "node scripts/smokeCheck.js"
  },
  "bin": {
    "list-sim-candidates": "scripts/listSimCandidates.js"
  },
  "keywords": [
    "cs2",
    "matchmaking",
    "steam",
    "discord",
    "esports"
  ],
  "author": "Romish.gg Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "basic-ftp": "^5.0.5",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "openid": "^2.0.12",
    "passport": "^0.7.0",
    "passport-steam": "^1.0.18",
    "rcon-srcds": "^2.1.0",
    "socket.io": "^4.6.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4"
  }
}
